@{ var etapa1 = @Request.QueryString["etapa"];

}




@if (etapa1 == "1")
{
    ViewBag.titulo = "SOLICITUDES SIMULADA";
}
else if (etapa1 == "2")
{
    ViewBag.titulo = "SOLICITUDES SOLICITADAS";
}
else if (etapa1 == "3")
{
    ViewBag.titulo = "SOLICITUDES CURSADAS";
}
else if (etapa1 == "4")
{
    ViewBag.titulo = "PENDIENTE DE CARGA EN CORFO";
}



@Html.Editor("perfil", new { htmlAttributes = new { @class = "form-control", @Value = ViewBag.perfil, @readonly = "readonly", @style = "display:none" } })

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h3 class="panel-title"> @ViewBag.titulo </h3>
            </div>
            <div class="panel-body">
                <table id="ListOpeEtp" class="table table-striped table-bordered" cellspacing="0" style="width:97%">
                    <thead>
                        <tr>
                            <th>Programa</th>
                            <th>Fecha Curse</th>
                            <th>Rut Empresa</th>
                            <th>Razón Social</th>
                            <th>Numero IFI</th>
                            <th>Ejecutivo</th>
                            <th>Oficina</th>
                            <th>Anexos</th>
                            <th>Acciones</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</div>




<!-- Modal -->
<div class="modal fade" id="abre_documento" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header bg-primary">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel1">DESCARGAR DOCUMENTOS </h4>
            </div>
            <div class="modal-body" id="vistaanexos"></div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>


